{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "bundler",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "allowJs": true,
      "declaration": true,
      "lib": ["DOM", "ESNext"],
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "useDefineForClassFields": false
    }
  }
  